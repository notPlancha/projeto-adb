\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{\{}
    \DataTypeTok{$lookup}\OperatorTok{:}\NormalTok{\{}
      \DataTypeTok{from}\OperatorTok{:} \StringTok{"players"}\OperatorTok{,}
      \DataTypeTok{localField}\OperatorTok{:} \StringTok{"loser"}\OperatorTok{,}
      \DataTypeTok{foreignField}\OperatorTok{:} \StringTok{"playerName"}\OperatorTok{,}
      \DataTypeTok{as}\OperatorTok{:} \StringTok{"loserInfo"}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{\{}
    \DataTypeTok{$project}\OperatorTok{:}\NormalTok{\{}
      \DataTypeTok{\_id}\OperatorTok{:} \DecValTok{0}\OperatorTok{,}
      \DataTypeTok{winner}\OperatorTok{:} \DecValTok{1}\OperatorTok{,}
      \DataTypeTok{loser}\OperatorTok{:} \DecValTok{1}\OperatorTok{,}
      \DataTypeTok{countWinner}\OperatorTok{:}\NormalTok{ \{ }\DataTypeTok{$size}\OperatorTok{:} \StringTok{"$winnerInfo"}\NormalTok{ \}}\OperatorTok{,}
      \DataTypeTok{countLooser}\OperatorTok{:}\NormalTok{ \{ }\DataTypeTok{$size}\OperatorTok{:} \StringTok{"$loserInfo"}\NormalTok{ \}}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{\{}
    \DataTypeTok{$match}\OperatorTok{:}\NormalTok{ \{}
      \DataTypeTok{$or}\OperatorTok{:}\NormalTok{ [}
\NormalTok{        \{}\DataTypeTok{countWinner}\OperatorTok{:} \DecValTok{0}\NormalTok{\}}\OperatorTok{,}
\NormalTok{        \{}\DataTypeTok{countLooser}\OperatorTok{:} \DecValTok{0}\NormalTok{\}}
\NormalTok{      ]}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{\{}
    \DataTypeTok{$project}\OperatorTok{:}\NormalTok{ \{}
      \DataTypeTok{player}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$cond}\OperatorTok{:}\NormalTok{ [\{}\DataTypeTok{$eq}\OperatorTok{:}\NormalTok{ [}\StringTok{"$countWinner"}\OperatorTok{,} \DecValTok{0}\NormalTok{]\}}\OperatorTok{,} \StringTok{"$winner"}\OperatorTok{,} \StringTok{"$loser"}\NormalTok{]\}}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}
\NormalTok{  \{}
    \DataTypeTok{$group}\OperatorTok{:}\NormalTok{ \{}
