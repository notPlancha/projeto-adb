\begin{Shaded}
\begin{Highlighting}[]
\NormalTok{db}\OperatorTok{.}\AttributeTok{matches}\OperatorTok{.}\FunctionTok{aggregate}\NormalTok{([}
\NormalTok{  \{}
    \DataTypeTok{$lookup}\OperatorTok{:}\NormalTok{\{}
      \DataTypeTok{from}\OperatorTok{:} \StringTok{"players"}\OperatorTok{,}
