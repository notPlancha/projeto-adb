      \DataTypeTok{Oponent}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$ne}\OperatorTok{:} \StringTok{"bye"}\NormalTok{\}}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}
\NormalTok{  \{}
    \DataTypeTok{$set}\OperatorTok{:}\NormalTok{ \{}
      \DataTypeTok{winner}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$cond}\OperatorTok{:}\NormalTok{ [\{}\DataTypeTok{$eq}\OperatorTok{:}\NormalTok{ [}\StringTok{"$WL"}\OperatorTok{,} \StringTok{"W"}\NormalTok{]\}}\OperatorTok{,} \StringTok{"$PlayerName"}\OperatorTok{,} \StringTok{"$Oponent"}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{loser}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$cond}\OperatorTok{:}\NormalTok{ [\{}\DataTypeTok{$eq}\OperatorTok{:}\NormalTok{ [}\StringTok{"$WL"}\OperatorTok{,} \StringTok{"W"}\NormalTok{]\}}\OperatorTok{,} \StringTok{"$Oponent"}\OperatorTok{,} \StringTok{"$PlayerName"}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{winnerLink}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$cond}\OperatorTok{:}\NormalTok{ [}
\NormalTok{        \{}\DataTypeTok{$eq}\OperatorTok{:}\NormalTok{ [}\StringTok{"$WL"}\OperatorTok{,} \StringTok{"W"}\NormalTok{]\}}\OperatorTok{,}
\NormalTok{        \{}\DataTypeTok{$split}\OperatorTok{:}\NormalTok{ [}\StringTok{"$LinkPlayer"}\OperatorTok{,} \StringTok{"/"}\NormalTok{]\}}\OperatorTok{,}
        \KeywordTok{null}
\NormalTok{      ]\}}\OperatorTok{,}
      \DataTypeTok{looserLink}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$cond}\OperatorTok{:}\NormalTok{ [}
\NormalTok{        \{}\DataTypeTok{$eq}\OperatorTok{:}\NormalTok{ [}\StringTok{"$WL"}\OperatorTok{,} \StringTok{"W"}\NormalTok{]\}}\OperatorTok{,}
        \KeywordTok{null}\OperatorTok{,} 
\NormalTok{        \{}\DataTypeTok{$split}\OperatorTok{:}\NormalTok{ [}\StringTok{"$LinkPlayer"}\OperatorTok{,} \StringTok{"/"}\NormalTok{]\}}
\NormalTok{      ]\}}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}
\NormalTok{  \{}
    \DataTypeTok{$group}\OperatorTok{:}\NormalTok{ \{}
      \DataTypeTok{\_id}\OperatorTok{:}\NormalTok{[}\StringTok{"$Tournament"}\OperatorTok{,} \StringTok{"$GameRound"}\OperatorTok{,} \StringTok{"$Date"}\OperatorTok{,} \StringTok{"$winner"}\OperatorTok{,} \StringTok{"$loser"}\OperatorTok{,}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$winnerLink"}\OperatorTok{,} \DecValTok{6}\NormalTok{]\}}\OperatorTok{,}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$looserLink"}\OperatorTok{,} \DecValTok{6}\NormalTok{]\}]}\OperatorTok{,}
      \DataTypeTok{count}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$sum}\OperatorTok{:} \DecValTok{1}\NormalTok{\}}\OperatorTok{,}
      \DataTypeTok{sets}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$push}\OperatorTok{:}\StringTok{"$Score"}\NormalTok{\}}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{\{}
    \DataTypeTok{$project}\OperatorTok{:}\NormalTok{ \{}
      \DataTypeTok{\_id}\OperatorTok{:} \KeywordTok{false}\OperatorTok{,}
      \DataTypeTok{tournament}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{0}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{gameRound}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{1}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{date}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{2}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{winner}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{3}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{loser}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{4}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{winnerLinkId}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{5}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{loserLinkId}\OperatorTok{:}\NormalTok{ \{}\DataTypeTok{$arrayElemAt}\OperatorTok{:}\NormalTok{ [}\StringTok{"$\_id"}\OperatorTok{,} \DecValTok{6}\NormalTok{]\}}\OperatorTok{,}
      \DataTypeTok{count}\OperatorTok{:} \KeywordTok{true}\OperatorTok{,}
      \DataTypeTok{sets}\OperatorTok{:} \KeywordTok{true}
\NormalTok{    \}}
\NormalTok{  \}}\OperatorTok{,}\NormalTok{ \{}
    \DataTypeTok{$out}\OperatorTok{:} \StringTok{"matches"}
\NormalTok{  \}}
\NormalTok{])}\OperatorTok{;}
\end{Highlighting}
\end{Shaded}

Este processo continua com a semelhança, adicionando a \emph{pipeline}
\texttt{\$match} para filtrar os jogos sem oponentes, e a
\emph{pipeline} \texttt{\$set} para criar as colunas \emph{winner} e
\emph{loser}. É adicionado a pipeline \texttt{\$match} é usada para
filtrar os jogos não jogados, e a \emph{pipeline} \texttt{\$cond}, o que
nos deixa escrever uma condição que decide sobre o vencedor e o
perdedor. A \emph{pipeline} \texttt{\$sets} garante que os nossos jogos
repitidos são realmente repetidos, sendo que um score de, por exemplo,
``67 72 25'' seria igual a um score de ``76 27 52'', devido à natureza
da coluna; esta vai incluir as varias formas como a coluna se encontra
nos vários jogos repetidos.

